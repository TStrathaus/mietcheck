# Database (Vercel Postgres - get from vercel.com after deployment)
POSTGRES_URL="postgres://default:xxx@xxx.postgres.vercel-storage.com:5432/verceldb"

# Google Gemini API (get from aistudio.google.com)
GEMINI_API_KEY="xxx"

# Stripe (get from stripe.com)
# Publishable key (starts with pk_)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY="pk_test_xxx"
# Secret key (starts with sk_)
STRIPE_SECRET_KEY="sk_test_xxx"
# Webhook secret (starts with whsec_) - get from Stripe Dashboard > Webhooks
STRIPE_WEBHOOK_SECRET="whsec_xxx"

# App URL (production URL after deployment)
NEXT_PUBLIC_APP_URL="http://localhost:3000"

# NextAuth Secret (generate with: openssl rand -base64 32)
NEXTAUTH_SECRET="your-random-secret-here"
NEXTAUTH_URL="http://localhost:3000"

# Vercel Blob Storage (optional, for file uploads)
BLOB_READ_WRITE_TOKEN="xxx"
