{
  "headers": [
    {
      "source": "/dashboard",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, max-age=0"
        }
      ]
    },
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "functions": {
    "src/app/api/upload-url/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    },
    "src/app/api/process-upload/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    },
    "src/app/api/analyze-contract/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    },
    "src/app/api/analyze-anpassung/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    }
  }
}
